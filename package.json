{
  "name": "@yggdrasilts/axiosfit",
  "version": "0.6.1",
  "author": "David Lopez <davidlopez.david@gmail.com>",
  "contributors": [
    "David Lopez <davidlopez.david@gmail.com>"
  ],
  "license": "MIT",
  "description": "Project inspired in Retrofit.",
  "keywords": [
    "typescript",
    "axios",
    "rxjs",
    "promise",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/yggdrasilts/axiosfit/issues"
  },
  "homepage": "https://github.com/yggdrasilts/axiosfit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yggdrasilts/axiosfit.git"
  },
  "scripts": {
    "allure:clean": "rm -rf allure-*",
    "allure:serve": "npm test && allure serve",
    "build": "npm run clean && npm run lint && npm test && tsc -p tsconfig.prod.json",
    "clean": "rm -rf allure* coverage dist documentation",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "compodoc": "npx compodoc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:samples": "prettier --write \"samples/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:test": "tslint -p tsconfig.test.json -c tslint.json",
    "prepublish": "npm run build",
    "test": "rm -rf coverage && npm run allure:clean && tsc -p tsconfig.test.json && DEBUG=testcontainers jest --config ./config/jest.config.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "allure-commandline": "^2.13.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "jest-allure": "^0.1.1",
    "prettier": "^1.19.1",
    "testcontainers": "^2.2.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">= 12.3.0",
    "npm": ">= 6.9.0"
  },
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  }
}
