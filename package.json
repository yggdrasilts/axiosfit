{
  "name": "@yggdrasilts/axiosfit",
  "version": "0.1.0-alpha.3",
  "author": "David Lopez <davidlopez.david@gmail.com>",
  "contributors": [
    "David Lopez <davidlopez.david@gmail.com>"
  ],
  "license": "MIT",
  "description": "Project inspired in Retrofit.",
  "keywords": ["typescript", "axios"],
  "bugs": {
    "url": "https://gitlab.com/yggdrasilts/axiosfit/issues"
  },
  "homepage": "https://gitlab.com/yggdrasilts/axiosfit",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/yggdrasilts/axiosfit.git"
  },
  "scripts": {
    "allure:clean": "rm -rf allure-*",
    "allure:serve": "npm test && allure serve",
    "build": "npm run clean && npm test && tsc -p tsconfig.prod.json",
    "clean": "rm -rf allure* coverage dist documentation",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "compodoc": "npx compodoc -p tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "test": "rm -rf coverage && npm run allure:clean && tsc -p tsconfig.test.json && DEBUG=testcontainers jest --config ./config/jest.config.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "allure-commandline": "^2.9.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "jest-allure": "^0.1.1",
    "prettier": "^1.18.2",
    "testcontainers": "^1.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 12.3.0",
    "npm": ">= 6.9.0"
  },
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  }
}
